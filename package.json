{
  "name": "etcher.io",
  "description": "react based static site",
  "dependencies": {
    "arch": "^2.1.0",
    "bootstrap": "4.0.0-alpha.6",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "raven-js": "3.15.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-markdown": "2.5.0",
    "react-transition-group": "^1.1.3",
    "react-twitter-widgets": "1.3.0",
    "react-typekit": "1.0.8",
    "reactstrap": "4.6.2",
    "resin-event-log": "^1.6.0",
    "sniffr": "^1.1.4"
  },
  "scripts": {
    "build:data": "babel-node lib/fetchData.js",
    "start": "next && node-sass -w --include-path scss styles/index.scss static/index.css",
    "build": "npm run build:data && npm run build-css && npm run build-banner && next build",
    "build-css": "node-sass --include-path scss styles/index.scss static/index.css --output-style compressed",
    "build-banner": "node-sass --include-path scss styles/success-banner.scss static/success-banner.css --output-style compressed",
    "serve": "next start",
    "export": "npm run build && next export -o build && cp CNAME build/CNAME && cp .nojekyll build/.nojekyll",
    "serve-build": "node ./test/index.js",
    "test": "ENV=test node ./test/index.js",
    "deploy": "gh-pages -d build --dotfiles"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.1",
    "babel-plugin-module-resolver": "2.7.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-wrap-in-js": "1.1.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "compression": "1.6.2",
    "eslint": "^3.19.0",
    "express": "4.15.2",
    "gh-pages": "^0.12.0",
    "glob": "7.1.1",
    "next": "beta",
    "node-sass": "4.5.2",
    "postcss-easy-import": "2.0.0",
    "postcss-loader": "1.3.3",
    "pwmetrics": "^2.0.0",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.3",
    "semver": "5.3.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "xml2js": "0.4.17"
  },
  "license": "Apache-2.0"
}
