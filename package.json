{
  "name": "etcher.io",
  "description": "react based static site",
  "dependencies": {
    "arch": "^2.1.0",
    "bootstrap": "4.0.0-alpha.6",
    "isomorphic-fetch": "2.2.1",
    "jsonp": "^0.2.1",
    "lodash": "4.17.4",
    "raven-js": "3.17.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-headroom": "^2.2.2",
    "react-icons": "^2.2.7",
    "react-markdown": "2.5.0",
    "react-transition-group": "^2.2.0",
    "react-twitter-widgets": "1.4.0",
    "react-typekit": "1.1.0",
    "reactstrap": "4.8.0",
    "resin-event-log": "^1.7.1",
    "sniffr": "^1.1.4"
  },
  "scripts": {
    "build:data": "babel-node lib/fetchData.js",
    "start": "next && node-sass -w --include-path scss styles/index.scss static/index.css",
    "build": "npm run build:data && npm run build-css && next build",
    "build-css": "node-sass --include-path scss styles/index.scss static/index.css --output-style compressed && node-sass --include-path scss styles/success-banner.scss static/success-banner.css --output-style compressed",
    "serve": "next start",
    "export": "npm run build && next export -o build && cp CNAME build/CNAME && cp .nojekyll build/.nojekyll",
    "serve-build": "node ./test/index.js",
    "test": "ENV=test node ./test/index.js",
    "deploy": "gh-pages -d build --dotfiles",
    "prettier": "prettier --single-quote --write '{lib,pages}/**/*.js'"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-cli": "6.26.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "compression": "1.7.0",
    "express": "4.15.4",
    "gh-pages": "^1.0.0",
    "github": "9.3.1",
    "glob": "7.1.2",
    "next": "beta",
    "node-sass": "4.5.3",
    "postcss-easy-import": "2.1.0",
    "postcss-loader": "2.0.6",
    "prettier": "1.5.3",
    "pwmetrics": "^3.1.2",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "license": "Apache-2.0"
}
